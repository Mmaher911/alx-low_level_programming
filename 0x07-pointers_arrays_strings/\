#include "main.h"
#include <stdio.h>

/**
 * print_diagsums - description
 * @size: size of array
 * @a: array on int
 * Return: nothing.
*/

void print_diagsums(int *a, int size)
{
	int itr, sl = 0, s2 = 0;

	for (itr = 0; itr < size; itr++)
	{
		s1 += a[itr];
		s2 += s[size - itr - 1];
		a += size;
	}
	printf("%d, ", s1);
	printf("%d\n", s2);
}
